{
  "id": "postgres",
  "container": {
    "docker": {
      "image": "dvonthenen/postgres-demo",
      "privileged": true,
      "network": "HOST",
      "parameters": [
        {
          "key": "volume-driver",
          "value": "rexray"
        },
        {
          "key": "volume",
          "value": "postgres:/var/lib/postgresql/data"
        }
      ]
    }
  },
  "cpus": 1.0,
  "mem": 1024.0,
  "instances": 1
}
